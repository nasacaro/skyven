body {
  margin: 0;
  padding: 0;
}

a,
a:hover,
a:focus {
  text-decoration: none;
}

h1,
h2,
h3,
h4,
h5,
h6 {
  font-family: ExoSB;
}

h2 {
  font-size: 36px;
}

p {
  font-family: Tahoma;
}

.team_para {
  text-align: center;
  color: #696969;
  text-transform: uppercase;
  opacity: 1;
  letter-spacing: 1.45px;
  font-size: 15px;
  line-height: 18px;
  position: relative;
  display: inline-block;
}
.team_para::before {
  content: "";
  position: absolute;
  left: -30px;
  border: 8px solid #E0821F;
  border-radius: 50%;
}

.slide {
  position: relative;
  margin-bottom: 50px;
}

.advisors .ouradvisor_repeater .team_image_wrapper {
  width: 245px;
  height: 357px;
  max-width: 100%;
  overflow: hidden;
  margin: 0 auto;
  background: rgba(217, 118, 52, 0.9);
}
.advisors .ouradvisor_repeater .team_image_wrapper .overlay p.info {
  font-size: 12px;
  min-height: 175px;
  max-height: 175px;
  overflow-y: auto;
  line-height: 24px;
  letter-spacing: 0.11px;
}
.advisors .ouradvisor_repeater .team_image_wrapper .member_data .member_name {
  font-size: 18px;
}

.ourteam_repeater .slide .team_image_wrapper {
  width: 346px;
  height: 433px;
  overflow: hidden;
  margin: 0 auto;
  max-width: 100%;
  background: rgba(217, 118, 52, 0.9);
}

.team_image_wrapper {
  position: relative;
  width: 100%;
  cursor: pointer;
}
.team_image_wrapper img {
  width: 100%;
  height: 100%;
  object-fit: cover;
  object-position: top;
}
.team_image_wrapper .overlay {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #008CBA;
  overflow: hidden;
  width: 100%;
  height: 0%;
  background: transparent linear-gradient(179deg, #E0821F 0%, #BE4B0D 100%) 0% 0% no-repeat padding-box;
  opacity: 0.85;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
}
.team_image_wrapper .overlay p.info {
  flex: 0 0 100%;
  margin: 0;
  padding: 10px 0;
  border-top: 1px solid #ffffff;
  border-bottom: 1px solid #ffffff;
  margin-top: 10px;
  color: #ffffff;
  text-align: center;
  font-size: 18px;
  min-height: 315px;
  max-height: 315px;
  overflow-y: auto;
  letter-spacing: 0.16px;
  line-height: 28px;
}
.team_image_wrapper .overlay .member_data {
  flex: 0 0 90%;
}
.team_image_wrapper .overlay .member_data p {
  color: #ffffff;
  margin: 0;
}
.team_image_wrapper .overlay .member_data p.member_name {
  font-size: 23px;
}
.team_image_wrapper .overlay .linked_link a {
  color: #ffffff;
}
.team_image_wrapper:hover .overlay {
  height: 100%;
  padding: 15px;
}
.team_image_wrapper:hover .member_data_wrapper {
  display: none;
}

.member_data_wrapper {
  position: absolute;
  background: transparent linear-gradient(283deg, #E0821F 0%, #BE4B0D 100%) 0% 0% no-repeat padding-box;
  display: flex;
  justify-content: space-around;
  align-items: center;
  width: 100%;
  bottom: -1px;
  padding: 5px 15px;
}
.member_data_wrapper .member_data {
  color: #ffffff;
  opacity: 1;
  font-size: 23px;
  width: 100%;
}
.member_data_wrapper .member_data .member_name {
  font-family: Tahoma;
  font-weight: 400;
  margin: 0;
}
.member_data_wrapper .linked_link i.fab {
  color: #ffffff;
  padding: 5px;
  cursor: pointer;
  border-radius: 50%;
}
.member_data_wrapper .linked_link i.fab:hover {
  background-color: #CD641A;
  border: 1px solid #CD641A;
}

.member_designation {
  font-weight: 400;
  font-size: 16px;
  margin-bottom: 5px;
}

.team_headerimage {
  font-family: ExoM;
  font-weight: 500;
  color: #42464B;
  font-size: 36px;
  text-align: center;
  letter-spacing: 0.33px;
  line-height: 43px;
  opacity: 1;
  position: relative;
  padding-bottom: 40px;
  margin-top: 60px;
}
.team_headerimage::after {
  position: absolute;
  height: 1px;
  border-bottom: 1px solid #3F3C3C;
  top: 53px;
  width: 80px;
  content: "";
  left: 0;
  right: 0;
  margin: auto;
  display: inline-block;
  opacity: 1;
}

.team_image_wrapper .member_data_wrapper .linked_link a.info-icon {
  display: none;
}

@media all and (min-width: 992px) and (max-width: 1199px) {
  .ouradvisor_repeater .member_data_wrapper {
    min-height: 101px;
  }

  .advisors .ouradvisor_repeater .team_image_wrapper .member_data .member_name {
    font-size: 17px;
  }

  .ouradvisor_repeater .col-lg-3 {
    flex: 0 0 33.33%;
    max-width: 33.33%;
  }
}
@media all and (max-width: 1024px) {
  .member_data_wrapper {
    padding: 5px 9px;
  }
  .member_data_wrapper .member_data {
    font-size: 18px;
    flex: 0 0 75%;
  }
  .member_data_wrapper .member_data .member_designation {
    font-size: 14px;
  }

  .team_image_wrapper:hover .overlay {
    height: calc(100% - 60px);
    padding: 0;
  }
  .team_image_wrapper:hover .member_data_wrapper {
    display: flex;
  }
  .team_image_wrapper .member_data_wrapper .linked_link {
    display: flex;
    align-items: center;
  }
  .team_image_wrapper .member_data_wrapper .linked_link a.info-icon {
    display: flex;
    width: 22px;
    height: 22px;
    align-items: center;
    justify-content: center;
    color: #ffffff;
    border-radius: 50%;
    border: 1px solid #ffffff;
    margin-left: 5px;
    font-size: 12px;
  }
  .team_image_wrapper .overlay {
    top: 0%;
    z-index: 1;
    width: 0;
    height: calc(100% - 60px);
  }
  .team_image_wrapper .overlay.show-info {
    width: 100%;
    padding: 10px;
    right: 10%;
    z-index: 1;
    height: calc(100% - 60px);
  }
  .team_image_wrapper .overlay .member_data,
.team_image_wrapper .overlay .linked_link {
    display: none;
  }
  .team_image_wrapper .overlay p.info {
    min-height: 295px;
    max-height: 295px;
  }

  .advisors .ouradvisor_repeater .team_image_wrapper {
    width: 306px;
    height: 393px;
  }
  .advisors .ouradvisor_repeater .team_image_wrapper .overlay.show-info {
    height: calc(100% - 83px);
  }
  .advisors .ouradvisor_repeater .team_image_wrapper .overlay p.info {
    font-size: 14px;
    min-height: 200px;
    max-height: 200px;
  }
}
.top-header {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  z-index: 999;
  text-align: center;
  padding: 15px 0px;
  background-color: #ffffff;
}
.top-header.fixed-header {
  position: fixed;
  width: 100%;
  float: left;
  padding: 10px 0;
  background: #ffffff;
  box-shadow: 0px 1px 3px #86450a;
  border-radius: 0;
}
.top-header .header-row {
  align-items: center;
}
.top-header .header-row nav.navbar {
  padding: 0;
}
.top-header .header-row nav.navbar ul.navbar-nav li a {
  font-size: 16px;
  color: #222222;
  padding: 5px 0;
  margin: 0 15px;
  border-bottom: 2px solid transparent;
  font-family: Tahoma;
}
.top-header .header-row nav.navbar ul.navbar-nav li a:hover {
  color: #FF5600;
  border-bottom: 2px solid #FF5600;
}
.top-header .header-row nav.navbar ul.navbar-nav li:first-child a {
  margin-left: 0;
}
.top-header .header-row nav.navbar ul.navbar-nav li:last-child a {
  /* margin-right: 0; */
}

.top-header .header-row nav.navbar ul.custom-navbar li a {
  font-size: 16px;
  color: #222222;
  padding: 5px 0;
  margin: 0 0 0 15px !important;
  border-bottom: 2px solid transparent;
  font-family: Tahoma;
}
.top-header .header-row nav.navbar ul.custom-navbar li:before {
	content: '';
    height: 30px;
    left: 0px;
    top: 50%;
    -webkit-transform: translateY(-50%);
    -ms-transform: translateY(-50%);
    transform: translateY(-50%);
    width: 1px;
    background: #717171;
    position: absolute;
}
.top-header .header-row nav.navbar ul.custom-navbar li a:hover {
	color: #222222;
	border-bottom: 2px solid transparent;
}

.top-header .header-row nav.navbar ul.navbar-nav li.current-menu-item a {
  color: #FF5600;
}

@media all and (max-width: 991px) {
  .top-header .header-row nav.navbar ul.navbar-nav {
    width: 100%;
    justify-content: flex-end;
  }
  .top-header .header-row a.site-logo {
    float: left;
  }
}
@media all and (max-width: 767px) {
  .top-header .header-row {
    justify-content: space-between;
  }
  .top-header .header-row .col-lg-4:first-child {
    display: none;
  }
  .top-header .header-row > div {
    display: inline-flex;
    width: auto;
    position: static;
  }
  .top-header .header-row nav.navbar {
    position: static;
  }
  .top-header .header-row nav.navbar button {
    padding: 0;
    outline: none;
  }
  .top-header .header-row nav.navbar button .hamburger span.hamburger-icon {
    display: block;
    width: 30px;
    height: 2px;
    background: #ef6236;
    margin: 0 auto 5px auto;
  }
  .top-header .header-row nav.navbar button .hamburger span.hamburger-icon:last-child {
    margin: 0;
  }
  .top-header .header-row nav.navbar div#collapsibleNavbar {
    position: absolute;
    top: 100%;
    width: 100%;
    left: 0;
    background: #dd7c36;
  }
  .top-header .header-row nav.navbar div#collapsibleNavbar ul.navbar-nav {
    width: 100%;
    justify-content: flex-end;
    max-width: 540px;
    margin: 0 auto;
    padding: 0 15px;
  }
  .top-header .header-row nav.navbar div#collapsibleNavbar ul.navbar-nav li a {
    margin: 0;
    text-align: left;
    padding: 5px;
    border-bottom: 1px solid #d2d2d2;
    color: #ffffff;
    font-weight: 400;
  }
  .top-header .header-row nav.navbar div#collapsibleNavbar ul.navbar-nav li:last-child a {
    border: 0;
  }
  .top-header .header-row nav.navbar div#collapsibleNavbar ul.navbar-nav li.current-menu-item a {
    font-weight: 600;
  }
}
@media all and (max-width: 575px) {
  .top-header .header-row nav.navbar div#collapsibleNavbar ul.navbar-nav {
    max-width: 100%;
  }
  .top-header .header-row nav.navbar button .hamburger span.hamburger-icon {
    width: 25px;
  }
  .top-header .header-row a.site-logo img {
    width: 200px;
  }
}
